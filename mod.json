{
    "Name": "nutone.server",
    "Description": "Gather kill statistics and sends them to the Nutone API.",
    "Version": "1.0.0",
    "LoadPriority": 1,
    "RequiredOnClient": false,
    "ConVars": [
        {
            "Name": "nutone_host",
            "DefaultValue": "localhost:80"
        },
        {
            "Name": "nutone_token",
            "DefaultValue": "CHECK_NORTHSTAR_WIKI_INSTRUCTIONS_ABOUT_CONVARS"
        }
    ],
    "Scripts": [
        {
            "Path": "killstat.nut",
            "RunOn": "SERVER",
            "ServerCallback": {
                "After": "killstat_Init"
            }
        }
    ]
}
